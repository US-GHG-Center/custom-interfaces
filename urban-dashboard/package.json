{
  "name": "urban_dashboard",
  "version": "0.1.0",
  "homepage": ".",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/main.js",
  "source": "src/lib/index.ts",
  "module": "dist/module.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "targets": {
    "urban-dashboard-app": {
      "source": "public/index.html",
      "context": "browser"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^5.16.7",
    "@mui/x-charts": "^7.13.0",
    "@swc/helpers": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "chart.js": "^4.4.3",
    "d3": "^7.9.0",
    "font-awesome": "^4.7.0",
    "font-awesome-icons": "^1.6.0",
    "mapbox-gl": "^3.5.1",
    "material-ui-popup-state": "^5.1.2",
    "ncp": "^2.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-router-dom": "^6.24.1",
    "web-vitals": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.16.1"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.16.1",
    "dotenv-cli": "^8.0.0",
    "parcel": "2.15.2",
    "parcel-resolver-tspaths": "0.0.9",
    "@parcel/packager-raw-url": "2.15.2",
    "@parcel/packager-ts": "2.15.2",
    "@parcel/transformer-typescript-types": "2.15.2",
    "@parcel/transformer-webmanifest": "2.15.2",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "build": "dotenv -- node ./run-parcel.js build build",
    "serve": "dotenv -- node ./run-parcel.js serve",
    "start": "parcel",
    "build-lib": "parcel build ./src/lib/index.ts --dist-dir dist",
    "clean": "rm -rf node_modules dist  build yarn.lock .parcel-cache && yarn cache clean"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}