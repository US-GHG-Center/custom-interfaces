  "publishConfig": {
    "access": "public"
  },
  "main": "dist/main.js",
  "source": "src/lib/index.ts",
  "module": "dist/module.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "targets": {
    "goes-app": {
      "source": "public/index.html",
      "context": "browser"
    }
  },
  "homepage": ".",
  "dependencies": {
    "@date-io/moment": "^3.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/x-date-pickers": "^7.22.0",
    "@swc/helpers": "^0.5.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@turf/turf": "^7.1.0",
    "d3": "^7.9.0",
    "mapbox-gl": "^3.7.0",
    "mapboxgl-timeline": "^1.0.2",
    "moment": "^2.30.1",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^6.27.0",
    "styled-components": "^6.1.13",
    "uuid": "^11.0.3",
    "web-vitals": "^2.1.0"
  },
  "peerDependencies": {
    "@mui/material": "^6.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "build": "dotenv -- node ./run-parcel.js build build",
    "serve": "dotenv -- node ./run-parcel.js serve",
    "start": "parcel",
    "build-lib": "parcel build ./src/lib/index.ts --dist-dir dist",
    "clean": "rm -rf node_modules dist build yarn.lock .parcel-cache && yarn cache clean"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "devDependencies": {
    "@mui/material": "^6.1.6",
    "@parcel/packager-raw-url": "2.15.2",
    "@parcel/packager-ts": "2.15.2",
    "@parcel/transformer-typescript-types": "2.15.2",
    "@parcel/transformer-webmanifest": "2.15.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "dotenv-cli": "^8.0.0",
    "parcel": "2.15.2",
    "parcel-resolver-tspaths": "^0.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^4.4.2"
  }
}
